{
	"apiVersion": 2,
	"name": "metalation-calculator-wp/calculator",
	"version": "1.0.0",
	"title": "Metalation Calculator",
	"category": "widgets",
	"icon": "calculator",
	"description": "Calculator to predict metal occupancies in vivo. Fill in the title, and enter the default values to use for availability.",
	"supports": {
		"html": false
	},
	"textdomain": "metalation-calculator-wp",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./include/main.css",
	"attributes": {
		"id": {
			"type": "string"
		},
		"bmcVals": {
				"type": "object",
				"default": {
					"mg": 2.7e-3,
					"mn": 2.6e-6,
					"fe": 4.8e-8,
					"co": 2.5e-9,
					"ni": 1.8e-13,
					"cu": 1.2e-18,
					"zn": 1.19e-12
				}
		},
		"title": {
				"type": "string",
				"default": "Idealised <em>Salmonella</em>",
				"selector": "h3"
		}
	}
}
